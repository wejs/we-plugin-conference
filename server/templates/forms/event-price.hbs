<div class="event-price-field form-group {{#if error}}has-error has-feedback{{/if}}">
  <label for="{{fieldId}}">{{t fieldName}} {{#ifCond field.allowNull false}}*{{/ifCond}}</label>
  {{{field-help-error error}}}

  <input id="{{fieldId}}" type="number" name="{{fieldName}}" >

  <p class="help-block">{{t help}}</p>
  {{{field-help-error error}}}
</div>
<script>
$(function(){
  $('#{{fieldId}}').maskMoney();
});
</script>